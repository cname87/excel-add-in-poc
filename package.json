{
  "name": "ex2-angular",
  "version": "0.0.0",
  "scripts": {
    "prettier:all": "prettier --write \"./**/*.ts\" \"./**/*.js\"",
    "eslint:all": "eslint --ext .js,.ts .",
    "ngBuild:dev": "ng run ex2-angular:build",
    "ngBuild:prod": "ng run ex2-angular:build:production",
    "build:dev": "npm-run-all prettier:all eslint:all ngBuild:dev",
    "build:prod": "npm-run-all prettier:all eslint:all ngBuild:prod",
    "serve:dev": "ng run ex2-angular:serve --ssl",
    "serve:prod": "ng run ex2-angular:serve:production --ssl",
    "deploy": "./deploy.sh",
    "test": "ng test",
    "e2e": "ng e2e",
    "validate": "office-addin-manifest validate manifest.xml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.10",
    "@angular/common": "~11.2.10",
    "@angular/compiler": "~11.2.10",
    "@angular/core": "~11.2.10",
    "@angular/forms": "~11.2.10",
    "@angular/platform-browser": "~11.2.10",
    "@angular/platform-browser-dynamic": "~11.2.10",
    "@angular/router": "~11.2.10",
    "@microsoft/office-js-helpers": "^1.0.2",
    "@types/office-js": "^1.0.173",
    "office-ui-fabric-js": "^1.5.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.9",
    "@angular/cli": "~11.2.9",
    "@angular/compiler-cli": "~11.2.10",
    "@types/jasmine": "~3.6.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@types/node": "^14.14.41",
    "@types/office-runtime": "^1.0.17",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "npm-run-all": "^4.1.5",
    "office-addin-debugging": "^4.1.5",
    "office-addin-dev-certs": "^1.5.17",
    "office-addin-manifest": "^1.5.21",
    "office-toolbox": "^0.3.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.5"
  }
}
